{
  "addons": [
    "heroku-postgresql",
    "memcached"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    },
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "MEMCACHEDCLOUD_SERVERS": {
      "required": true
    },
    "MEMCACHEDCLOUD_USERNAME": {
      "required": true
    },
    "MEMCACHEDCLOUD_PASSWORD": {
      "required": true
    }
  },
  "formation": {
  },
  "name": "caching-test",
  "scripts": {
    "postdeploy": "rake db:migrate db:seed"
  }
}
